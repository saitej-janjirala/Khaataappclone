        android:supportsRtl="true"
        android:testOnly="false"
        android:theme="@style/AppTheme"
        tools:ignore="HardcodedDebugMode">
        <activity android:name=".sendmoney"></activity>
        <activity android:name=".Customerchat" />
        <activity android:name=".Customerchats" />
        <activity android:name=".Newkhaataentry" />
        <activity android:name=".UserContents" />
        <activity android:name=".Otp" />
        <activity android:name=".Enterphonenumber" />
        <activity android:name=".Language" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>